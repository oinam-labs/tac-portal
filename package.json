    "stylelint": "stylelint \"**/*.html\" --custom-syntax postcss-html",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "theme-audit": "powershell -ExecutionPolicy Bypass -File scripts/theme-audit.ps1",
    "theme-audit:ci": "grep -RInE \"(text|bg|border|ring)-(slate|gray)-\" components pages && exit 1 || exit 0"
